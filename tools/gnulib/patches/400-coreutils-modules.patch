+          ISAAC_MIX (s, a, b, c, d, e, f, g, h, s->m);
+        }
+    }
+  else
+    {
+      /* The no seed case (as in reference ISAAC code) */
+      for (i = 0; i < ISAAC_WORDS; i++)
+        s->m[i] = 0;
+    }
