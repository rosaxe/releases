+   should be seeded with nonce data before calling isaac_seed.  The
+   other members are private.  */
+struct isaac_state
+  {
+    isaac_word m[ISAAC_WORDS];	/* Main state array */
+    isaac_word a, b, c;		/* Extra variables */
+  };
